{
  "devDependencies": {
    "@lukaspolak/kebab-case": "0.0.2",
    "js-beautify": "1.14.6",
    "npm-watch": "0.11.0",
    "vitest": "0.25.3"
  },
  "name": "manifests-to-markdown",
  "scripts": {
    "coverage": "vitest run --coverage",
    "watch": "npm-watch",
    "emit:all": "node src/index.mjs",
    "test": "vitest"
  },
  "version": "1.0.0",
  "watch": {
    "emit:all": [
      "src/*.*",
      "manifests/metrist/*.*"
    ]
  }
}
